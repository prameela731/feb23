a=[]
n=int(input('enter n'))
for i in range(n):
    b=int(input())
    a.append(b)
min=max=a[0]
for i in range(1,n):
    if a[i]<min:
        min=a[i]
print("min=",min)
for i in range(1,n): 
    if a[i]>max:
        max=a[i]
print("max=",max)
